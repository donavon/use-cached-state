{
  "name": "use-cached-state",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.0",
  "description": "Know how long it's been since a user has \"seen\" your app.",
  "main": "dist/use-cached-state.js",
  "umd:main": "dist/use-cached-state.umd.js",
  "module": "dist/use-cached-state.m.js",
  "source": "src/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/donavon/use-cached-state.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "eslint src",
    "test": "jest --verbose --coverage --silent",
    "test:watch": "jest --watch --runInBand --silent",
    "prebuild": "npm run lint && npm t && rimraf dist",
    "build": "microbundle -o dist/ --sourcemap false --target web",
    "dev": "microbundle watch -o dist/ --sourcemap false --compress false"
  },
  "keywords": [
    "react-hooks",
    "hooks",
    "react",
    "utils",
    "lib",
    "state",
    "cache"
  ],
  "author": "Donavon West <github@donavon.com> (https://github.com/donavon)",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.10.0",
    "eslint-config-amex": "^9.0.0",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "microbundle": "^0.9.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-hooks-testing-library": "^0.3.8",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 75,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@use-it/instance": "^0.2.0"
  }
}
